inherit lua

DESCRIPTION="Lua documentation generator"
HOMEPAGE="http://luadoc.luaforge.net/"
SRC_URI="http://luaforge.net/frs/download.php/2557/${P}.tar.gz"

src_compile() { :; }

src_install() {
	cd ${S}
	cyginstall \
		LUA_DIR=${D}${LUA_SCRIPTDIR} \
		LAUNCHER_DIR=${D}/usr/bin
}

HTMLDOCS="doc/us/*.*"
